from tkinter import *

rt = Tk()

f2 = Frame(rt)
f1 = Frame(f2)


lbla = Label(f1, text='A')
lbla.pack(side='top')

lblb = Label(f1, text='B')
lblb.pack(side='top')

lblc = Label(f1, text='C')
lblc.pack(side='left')

lbld = Label(rt, text='D')
lbld.pack(side='top')

rt.mainloop()
